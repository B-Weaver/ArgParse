<ArgParser>
	<program>
		<programname>VolumeCalculatorD</programname>
		<description>Calculate the volume of any shape</description>
	</program>
	<positional>
		<name>length</name>
		<argdescription>the length of the shape</argdescription>
		<type>float</type>
		<position>1</position>
	</positional>
	<positional>
		<name>width</name>
		<argdescription>the width of the shape</argdescription>
		<type>float</type>
		<position>2</position>
	</positional>
	<positional>
		<name>height</name>
		<argdescription>the height of the shape</argdescription>
		<type>float</type>
		<position>3</position>
	</positional>
	<named>
		<name>type</name>
		<argdescription>the type of shape</argdescription>
		<shortname>t</shortname>
		<type>string</type>
		<default>box</default>
		<restricted>box</restricted>
		<restricted>ellipsoid</restricted>
		<restricted>pyramid</restricted>
	</named>
	<named>
		<name>digits</name>
		<argdescription>the number of digits</argdescription>
		<shortname>d</shortname>
		<type>integer</type>
		<default>4</default>
		<restricted>2</restricted>
		<restricted>3</restricted>
		<restricted>4</restricted>
		<restricted>8</restricted>
	</named>
</ArgParser>